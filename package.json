{
  "name": "petproj",
  "version": "1.0.0",
  "description": "exploring nodejs",
  "main": "index.js",
  "repository": "https://github.com/oreoluwade/petproj.git",
  "author": "oreoluwade",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "start": "npm run build && node ./dist",
    "start:dev": "nodemon -w server --exec node_modules/.bin/babel-node -- server",
    "build": "node_modules/.bin/babel ./server -d ./dist",
    "migrate": "node_modules/.bin/babel-node node_modules/.bin/sequelize db:migrate",
    "undo:migrate": "node_modules/.bin/babel-node node_modules/.bin/sequelize db:migrate:undo:all"
  },
  "dependencies": {
    "bcrypt": "^1.0.3",
    "body-parser": "^1.18.2",
    "colors": "^1.1.2",
    "dotenv": "^4.0.0",
    "eslint-import-resolver-node": "^0.3.1",
    "express": "^4.16.2",
    "morgan": "^1.9.0",
    "pg": "^7.4.0",
    "pg-hstore": "^2.3.2",
    "pryjs": "^1.0.3",
    "sequelize": "^4.23.2",
    "sequelize-cli": "^3.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.0.2",
    "babel-preset-env": "^1.6.1",
    "eslint": "^4.11.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.5.1",
    "nodemon": "^1.12.1"
  }
}
