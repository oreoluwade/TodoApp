{
  "extends": "airbnb-base",
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6
  },
  "plugins": [
    "import",
    "jsx-a11y",
    "react"
  ],
  "settings": {
    "import/resolver": {
      "node": {
        "extensions": [".js"],
        "paths": ["server"]
      }
    },
    "import/extensions": [".js"]
  },
  "rules": {
    "import/prefer-default-export": 0,
    "import/no-unresolved": 0
  }
}

